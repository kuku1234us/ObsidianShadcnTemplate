    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "postcss": "^8.4.49",
    "rollup": "^2.79.1",
